{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["vue", "jest"],
  "rules": {
    "no-irregular-whitespace": "warn",
    "no-undef": "warn"
  },
  "ignorePatterns": ["src/sm-resolver.js"]
}
